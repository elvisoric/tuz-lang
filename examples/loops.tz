// Loop examples in Forge

fn main() -> int {
    // For loop: sum 0 to 9
    let sum = 0;
    
    for i = 0, 10 {
        sum = sum + i;
    }
    
    // While loop: countdown
    let count = 5;
    while count > 0 {
        count = count - 1;
    }
    
    // Nested loops
    let total = 0;
    for i = 0, 5 {
        for j = 0, 5 {
            total = total + 1;
        }
    }
    
    return total;  // Returns 25
}
